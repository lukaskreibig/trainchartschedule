{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "next", "next/core-web-vitals", "prettier"
    ],

    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "prettier"
        ],
    "rules": {
        "prettier/prettier": ["error"],
        "@typescript-eslint/no-non-null-assertion": "off"
      }
}
